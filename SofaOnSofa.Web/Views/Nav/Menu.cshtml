@model IEnumerable<string>

@Html.ActionLink("Главная", "List", "Product")

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
        {
            controller = "Product",
            action = "List",
            style = link,
            page = 1
        },
        new
        {
            @class = link == ViewBag.SelectedStyle ? "selected" : null
        });
}
